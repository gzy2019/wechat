/* pages/home/home.wxss */
.top-title{
  width:100%;
  height:120rpx;
  margin:10rpx 0;
  display:flex;
  align-items:center;
  background:#ccc;
}
.title-name{
  width:85%;
  height:150rpx;
  padding:0 30rpx;
  display:flex;
  align-items:center;
}
.title-name image{
  width:100rpx;
  height:100rpx;
  border-radius:50%;
  margin-right:30rpx;
  background:url('data:image/png;base64,');
background-size:cover;
}
.baby-msg{
  width:35%;
  display:flex;
  justify-content:flex-start;
  align-items:flex-start;
  flex-wrap:wrap;
}
.baby-name{
  font-size:34rpx;
  font-weight:bold;
  color:#FF6B6B;
}
.baby-birthday{
  font-size:26rpx;
  color:#FF6B6B;
}
.addpic{
  width:15%;
  height:100rpx;
  display:flex;
  align-items:center;
}
.addpic image{
  width:50%;
  height:50%;
  margin:auto;
}
.mybanner{
  width:100%;
  height:400rpx;
}
swiper{
  width:100%;
  height:100%;
}
swiper-item{
  text-align: center;
  background:#000;
}
swiper-item image{
  width:100%;
  height:100%;
}
.img-show{
  display:flex;
  justify-content: flex-start;
  align-items:flex-start;
  background:#ddd;
  padding:10rpx;
}
.left-adorn{
  width:3%;
}
.middle-sign{
  width:100%;
  border-left:3rpx solid #FF6B6B;
  margin-left:50%;
}
.top-sign{
  width:15rpx;
  height:15rpx;
  margin:20rpx 0 10rpx 0;
  border:5rpx solid #FF6B6B;
  border-radius:50%;
}
.right-adorn{
  width:98%;
  margin-left:20rpx;
  background:#fff;
  border-radius:10rpx;
}
.baby-day,.up-date{
  height:40rpx;
  font-size:26rpx;
  color:#aaa;
  margin:15rpx 0 10rpx 35rpx;
}
.center-img{
  width:640rpx;
  margin:0 auto;
  text-align: center;
}
.up-date{
  display:block;
}